[includeIf "gitdir:~/.dotfiles"]
	[user]
		name = Proxemy
		email = 33573115+proxemy@users.noreply.github.com
	[status]
		showUntrackedFiles = no
	[remote "origin"]
		url = https://github.com/proxemy/dotfiles
		pushurl = git@github.com:proxemy/dotfiles.git

[core]
	editor = nvim
[alias]
	l = log --graph --oneline --relative-date --pretty='%C(yellow)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN%Cred%d %Creset%s'
	s = status -sb --ahead-behind
	d = diff --minimal
	co = checkout
[init]
	defaultBranch = main
